<%= render 'shared/header', header: "Login" %>

<%= form_tag '/login', method: 'post' do %>
   <%= label_tag :username %>
   <%= text_field_tag :username, params[:username] %>
   <br/>
   <%= label_tag :password%>
   <%= password_field_tag :password %>
   <br/>
   <%= submit_tag "login" %>
<% end %>